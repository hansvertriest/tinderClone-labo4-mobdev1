{
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "rules":{
        "linebreak-style": 0,
        "no-plusplus":"off",
        "class-methods-use-this":"off",
	  "no-tabs": "off",
	  "indent": ["error", "tab"],
	  "no-unused-vars": "off",
	  "max-len": "off",
	  "quote-props": "off"
    }
}